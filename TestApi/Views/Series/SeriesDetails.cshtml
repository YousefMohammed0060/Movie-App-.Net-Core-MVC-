@model Series

@{
    ViewBag.Title = Model.name;
}

<div class="row">
    <img src="@Model.backdrop_path" width="100%" alt="@Model.name">
    <h2><b>Name:</b> @Model.name</h2>
    <h4><b>Release Date:</b> @Model.first_air_date</h4>
    <h4><b>Overview:</b> @Model.overview</h4>
    <h4><b>Vote Avarage:</b> @Model.vote_average/10</h4>
</div>